{
    "name": "OctoPrint Next",
    "version": "0.0.8",
    "slug": "octoprint-next",
    "description": "New version of OctoPrint add-on for Home Assistant",
    "arch": ["armv7", "amd64", "armhf", "aarch64", "i386"],
    "url": "https://github.com/fredrikbaberg/hassio-addons",
    "startup": "application",
    "webui": "http://[HOST]:[PORT:5000]",
    "boot": "manual",
    "ports": {
        "5000/tcp": null
    },
    "ports_description": {
        "5000/tcp": "WebUI (Not required for Ingress)"
    },
    "devices": [
        "/dev/mem",
        "/dev/gpiomem"
    ],
    "homeassistant": "2021.4.0",
    "privileged": [
        "SYS_RAWIO"
    ],
    "map": [],
    "gpio": true,
    "usb": true,
    "uart": true,
    "options": {
        "webcam_url": "774437fd-ustreamer:80",
        "mjpg_input": "input_file.so -f /www_mjpg -e",
        "recovery": false
    },
    "schema": {
        "webcam_url": "str",
        "mjpg_input": "str",
        "recovery": "bool?"
    },
    "tmpfs": true,
    "ingress": true,
    "panel_icon": "mdi:printer-3d",
    "panel_title": "OctoPrint Next",
    "panel_admin": false,
    "snapshot_exclude": [],
    "stage": "experimental",
    "image": "fredrikbaberg/octoprint-next-{arch}-image"
}
